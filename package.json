  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --open --mode development",
    "build": "GENERATE_SOURCEMAP=false webpack --config webpack.production.js --color",
    "winBuild": "set \"GENERATE_SOURCEMAP=false\" && webpack --config webpack.production.js --color",
    "test": "jest --silent --env=jsdom --coverage --updateSnapshot --coverageDirectory",
    "otest": "cross-env CI=true react-app-rewired test --silent --env=jsdom --coverage --updateSnapshot --testResultsProcessor jest-sonar-reporter",
    "eject": "react-scripts eject",
    "sonar": "node sonar-project.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "format": "prettier --write \"src/**/*.{js,jsx,scss}\"",
    "lint": "eslint src/.",
    "lint:fix": "eslint --fix ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-transform-arrow-functions": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-stage-0": "^7.8.3",
    "autoprefixer": "^7.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "file-loader": "^6.2.0",
    "html-webpack-link-type-plugin": "^1.1.0",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.0.0",
    "mini-css-extract-plugin": "^0.11.3",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "path": "^0.12.7",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "sass": "^1.50.1",
    "sass-loader": "^7.3.1",
    "terser-webpack-plugin": "^4.2.3",
    "transfer-webpack-plugin": "^0.1.4",
    "url-loader": "^4.1.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "^6.1.1"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "TEST-RESULTS.xml",
    "indent": 4
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|scss|jsx)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testResultsProcessor": "./node_modules/jest-junit-reporter",
    "coverageReporters": [
      "text",
      "cobertura"
    ]
  }
}
